{
  "version": "2020-21-03",
  "content": {
    "^[_]*ПД[_]*$": {
      "description": "Проектная деятельность",
      "normalized": "ПД"
    },
    "^[_-]*(LMS|ЛМС)[_-]*$": {
      "description": "Обучение в LMS",
      "normalized": "LMS"
    },
    "^Обучение\\s+в\\s+LMS$": {
      "description": "Обучение в LMS",
      "normalized": "LMS"
    },
    "^Webex$": {
      "description": "Видеоконференция в Webex",
      "normalized": "Webex"
    },
    "^Webinar$": {
      "description": "Онлайн лекция в Webinar",
      "normalized": "Webinar"
    },
    "^м[\\s\\p{P}]*спорт[\\s\\p{P}]*зал[\\p{P}]*$": {
      "description": "Михалковская, Спортивный зал",
      "normalized": "М Спорт. зал"
    },
    "^Зал\\s+№*(\\d)[_]*$": {
      "description": "Спортивный зал №$1",
      "normalized": "Спорт. зал №$1"
    },
    "^Автозаводская\\s+(\\d)$": {
      "description": "Автозаводская, Спортивный зал №$1",
      "normalized": "Ав Спорт. зал №$1"
    },
    "^(.*Измайлово.*)$": {
      "description": "$1",
      "normalized": "$1"
    },
    "^ИМАШ(\\sРАН)?[\\s_]*$": {
      "description": "Институт машиноведения РАН",
      "normalized": "ИМАШ РАН"
    },
    "^МИСиС$": {
      "description": "НИТУ МИСиС",
      "normalized": "МИСиС"
    },
    "^м\\s*((\\d)(\\d).+)$": {
      "description": "Михалковская, к. $2, этаж $3, ауд. $1",
      "normalized": "М$1"
    },
    "^(\\d)пк\\s*((\\d).+)$": {
      "description": "Павла Корчагина, к. $1, этаж $3, ауд. $2",
      "normalized": "$1Пк$2"
    },
    "^пк\\s*((\\d).+)$": {
      "description": "Павла Корчагина, к. 1, этаж $2, ауд. $1",
      "normalized": "Пк$1"
    },
    "^А[\\s-]?ОМД$": {
      "description": "Б. Семёновская, к. А, Лаб. ОМД",
      "normalized": "А ОМД"
    },
    "^ав\\s*((\\d)(\\d)(.+))$": {
      "description": "Автозаводская, к. $2, этаж $3, ауд. $1",
      "normalized": "Ав$1"
    },
    "^пр\\s*((\\d)(\\d).+)$": {
      "description": "Прянишникова, к. $2, этаж $3, ауд. $1",
      "normalized": "Пр$1"
    },
    "^пр\\s(ФО[\\s-]*(\\d+))$": {
      "description": "Прянишникова, к. 2, этаж 4, ауд. $1",
      "normalized": "Пр ФО $2"
    },
    "^пр\\s*ВЦ\\s*(\\d+)\\s*\\(((\\d)(\\d).+)\\)$": {
      "description": "Прянишникова, к. $3, этаж $4, ауд. $2",
      "normalized": "Пр ВЦ $1"
    },
    "^([АБВН]|Нд)\\s*((\\d).+)$": {
      "description": "Б. Семёновская, к. $1, этаж $3, ауд. $0",
      "normalized": "$1$2"
    },
    "^H(д?)\\s*((\\d).+)$": {
      "description": "Б. Семёновская, к. Н$1, этаж $3, ауд. Н$1$2",
      "normalized": "Н$1$2"
    },
    "^ИОНХ(\\sРАН)?[\\s_]*$": {
      "description": "Институт общей и неорган. химии РАН",
      "normalized": "ИОНХ РАН"
    },
    "^(ВДНХ)?\\sТехноград$": {
      "description": "Техноград на ВДНХ",
      "normalized": "Техноград"
    },
    "^Центр \"Биоинженерии\" РАН$": {
      "description": "Центр «Биоинженерия» РАН",
      "normalized": "Биоинженерия РАН"
    },
    "^Практика$": {
        "description": "Практика",
        "normalized": "Практика"
    },
    "^Бизнес.кар$": {
        "description": "Группа компаний «БИЗНЕС КАР»",
        "normalized": "Бизнес Кар"
    }
  }
}